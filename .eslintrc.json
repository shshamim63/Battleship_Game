{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-undef": "off",
        "no-unused-vars": "off",
        "eol-last": "off",
        "no-restricted-globals": "off",
        "no-use-before-define": "off",
        "no-const-assign": "off",
        "import/extensions": "off",
        "import/no-cycle": "off",
        "import/prefer-default-export": "off"
    }
}